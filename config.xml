<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.phonegap.getting.started" version="1.5.0">

	<name>Mobile Sandbox</name>

	<description>Mobile Sandbox</description>

	<author href="http://webpro.nl" email="lars@webpro.nl">Lars Kappert</author>

	<preference name="target-device" value="universal" />
	<preference name="orientation" value="default"/>
	<preference name="fullscreen" value="false" />

	<preference name="webviewbounce" value="true" />
	<preference name="prerendered-icon" value="false" />

	<icon src="img/icon-57x57.png" gap:role="default"/>
	<icon src="img/icon-57x57.png" width="57" height="57"/>
	<icon src="img/icon-72x72.png" width="72" height="72"/>
	<icon src="img/icon-114x114.png" width="114" height="114"/>
	<icon src="img/icon-144x144.png" width="144" height="144"/>

	<gap:splash src="img/splash-320x480.png"/>
	<gap:splash src="img/splash-640x960.png" width="640" height="960"/>
	<gap:splash src="img/splash-768x1004.png" width="768" height="1004"/>
	<gap:splash src="img/splash-1024x748.png" width="1024" height="748"/>
	<gap:splash src="img/splash-1536x2008.png" width="1536" height="2008"/>
	<gap:splash src="img/splash-2048x1496.png" width="2048" height="1496"/>

</widget>
